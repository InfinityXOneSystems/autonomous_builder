{
  "name": "node-api-cloudrun",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "dev": "node --loader ts-node/esm src/server.ts",
    "contract:check": "node scripts/contract/check_contract.js",
    "prime:validate": "node --loader ts-node/esm scripts/prime/full-validation.ts",
    "prime:risk": "node --loader ts-node/esm scripts/prime/risk-gate.ts"
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
